@model Parser.Domain.Vacancy
@{ ViewBag.Title = "Добавление пользователя"; }
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="Position" class="control-label">Position</label>
        <input type="text" asp-for="Position" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Salary" class="control-label">Salary</label>
        <input type="text" asp-for="Salary" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Company" class="control-label">Company</label>
        <input type="text" asp-for="Company" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Details" class="control-label">Details</label>
        <input type="text" asp-for="Details" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>